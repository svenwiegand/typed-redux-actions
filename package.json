{
  "name": "typed-redux-actions",
  "version": "1.2.1",
  "description": "Typed internationalization (intl/i18n) library for TypeScript/JavaScript apps.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "jam": {
    "main": "./dist/index.browser.js"
  },
  "browser": {
    "dist/index.js": "./dist/index.browser.js"
  },
  "scripts": {
    "build": "webpack || echo not ok",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "coverage": "yarn test -- --coverage --no-cache",
    "coverage:watch": "yarn run coverage -- --watch",
    "prepareRelease": "yarn run build && yarn run coverage && yarn run doc && git status",
    "release": "standard-version",
    "doc": "typedoc src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svenwiegand/typed-redux-actions.git"
  },
  "keywords": [
    "typescript",
    "redux",
    "action",
    "reducer",
    "concise",
    "discriminated unions"
  ],
  "author": "Sven Wiegand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/svenwiegand/typed-redux-actions/issues"
  },
  "homepage": "https://github.com/svenwiegand/typed-redux-actions/issues",
  "dependencies": {
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@types/jest": "^21.1.1",
    "@types/node": "^8.0.31",
    "browserify": "^14.4.0",
    "delete-empty": "^1.0.1",
    "dts-bundle": "^0.7.3",
    "jest": "^20.0.4",
    "standard-version": "^4.2.0",
    "ts-jest": "^20.0.10",
    "ts-loader": "^3.1.0",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.9.0",
    "typescript": "~2.5.3",
    "webpack": "3.5.5",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "src/.*\\.test\\.ts$",
      "\\.d\\.ts$",
      "/node_modules/"
    ],
    "testRegex": "src/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
